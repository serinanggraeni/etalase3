generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id       Int     @id @default(autoincrement())
  tiktok   String
  shopee   String
  category String
  price    Float
  image    String
  createdAt DateTime @default(now())
}

model testimonials {
  id       Int     @id @default(autoincrement())
  name     String
  rating   Int
  message  String
  createdAt DateTime @default(now())
}